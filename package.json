{
  "name": "blog-gavinmogan-com",
  "private": true,
  "description": "My Website powered by gatsby",
  "version": "0.0.0",
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halkeye/blog-gavinmogan-com.git"
  },
  "bugs": {
    "url": "https://github.com/halkeye/blog-gavinmogan-com/issues"
  },
  "homepage": "https://blog.gavinmogan.com",
  "dependencies": {
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@mdx-js/mdx": "^2.1.5",
    "@mdx-js/react": "^2.1.5",
    "eslint": "^8.12.0",
    "eslint-config-react-app": "^7.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "font-awesome": "^4.7.0",
    "gatsby": "^5.2.0",
    "gatsby-plugin-canonical-urls": "^5.2.0",
    "gatsby-plugin-catch-links": "^5.2.0",
    "gatsby-plugin-feed": "^5.2.0",
    "gatsby-plugin-image": "^3.2.0",
    "gatsby-plugin-manifest": "^5.2.0",
    "gatsby-plugin-mdx": "^5.2.0",
    "gatsby-plugin-netlify": "^5.1.0",
    "gatsby-plugin-nprogress": "^5.2.0",
    "gatsby-plugin-react-helmet": "^6.2.0",
    "gatsby-plugin-sharp": "^5.2.0",
    "gatsby-plugin-sitemap": "^6.2.0",
    "gatsby-remark-autolink-headers": "^6.2.0",
    "gatsby-remark-copy-linked-files": "^6.2.0",
    "gatsby-remark-embed-youtube": "^0.0.7",
    "gatsby-remark-emoji": "^0.0.3",
    "gatsby-remark-images": "^7.2.0",
    "gatsby-remark-prismjs": "^7.2.0",
    "gatsby-remark-relative-images": "^2.0.2",
    "gatsby-source-filesystem": "^5.2.0",
    "gatsby-transformer-remark": "^6.2.0",
    "gatsby-transformer-sharp": "^5.2.0",
    "gatsby-transformer-yaml": "^5.3.0",
    "lodash.kebabcase": "^4.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "truncate": "^3.0.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop -p 3000 -H 0.0.0.0",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "TZ=UTC gatsby build",
    "clean": "gatsby clean",
    "lint:js": "eslint  --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "remarkConfig": {
    "plugins": [
      [
        "frontmatter",
        {
          "type": "yaml",
          "marker": "-"
        }
      ],
      "remark-preset-lint-recommended",
      [
        "remark-lint-no-literal-urls",
        false
      ]
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/",
      "<rootDir>/public/"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "transformIgnorePatterns": [
      "node_modules/(?!(gatsby)/)"
    ],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/tests/__setup__/transformer.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/__setup__/frameworkSetup.js"
    ],
    "setupFiles": [
      "<rootDir>/tests/__setup__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "devDependencies": {
    "gatsby-awesome-pagination": "^0.3.8"
  }
}
