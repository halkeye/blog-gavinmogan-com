    "@material-ui/icons": "^3.0.1",
    "autobind-decorator": "^2.1.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^23.6.0",
    "babel-plugin-remove-graphql-queries": "^2.5.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "cli-glob": "^0.1.0",
    "enzyme": "^3.7.0",
    "enzyme-adapter-react-16": "^1.6.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.7.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-config-standard": "^12.0.0",
    "eslint-plugin-babel": "^5.2.1",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jest": "^21.25.1",
    "eslint-plugin-jsx-a11y": "^6.1.2",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^4.0.1",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-standard": "^4.0.0",
    "gatsby-plugin-favicon": "^3.1.4",
    "gatsby-plugin-robots-txt": "^1.3.0",
    "gatsby-plugin-sentry": "0.1.0",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-source-name": "^1.0.0",
    "history": "^4.7.2",
    "husky": "^1.1.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.6.0",
    "lint-staged": "^7.3.0",
    "lodash": "^4.17.11",
    "lodash-webpack-plugin": "^0.11.5",
    "markdownlint": "^0.11.0",
    "markdownlint-cli": "^0.13.0",
    "node-sass": "^4.9.4",
    "prettier": "^1.14.3",
    "prettier-eslint-cli": "^4.7.1",
    "prismjs": "^1.15.0",
    "react": "^16.5.2",
    "react-disqus-comments": "^1.2.0",
    "react-dom": "^16.5.2",
    "react-gist": "^1.2.1",
    "react-helmet": "^5.2.0",
    "react-jss": "^8.6.1",
