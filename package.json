  "devDependencies": {
    "gatsby": "^2.0.25",
    "gatsby-image": "^2.0.15",
    "gatsby-plugin-canonical-urls": "^2.0.6",
    "gatsby-plugin-catch-links": "^2.0.5",
    "gatsby-plugin-feed": "^2.0.8",
    "gatsby-plugin-google-analytics": "^2.0.6",
    "gatsby-plugin-manifest": "^2.0.5",
    "gatsby-plugin-netlify": "^2.0.1",
    "gatsby-plugin-nprogress": "^2.0.5",
    "gatsby-plugin-offline": "^2.0.7",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-sass": "^2.0.1",
    "gatsby-plugin-sharp": "^2.0.7",
    "gatsby-plugin-sitemap": "^2.0.1",
    "gatsby-plugin-twitter": "^2.0.6",
    "gatsby-remark-autolink-headers": "^2.0.7",
    "gatsby-remark-copy-linked-files": "^2.0.5",
    "gatsby-remark-images": "^2.0.4",
    "gatsby-remark-prismjs": "^3.0.2",
    "gatsby-remark-responsive-iframe": "^2.0.5",
    "gatsby-source-filesystem": "^2.0.4",
    "gatsby-transformer-remark": "^2.1.7",
    "gatsby-transformer-sharp": "^2.1.4"
  },
  "peerDependencies": {
    "gatsby": ">=1"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "precommit": "lint-staged",
    "develop": "gatsby develop -p 3000",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "TZ=UTC gatsby build",
    "build:pp": "TZ=UTC gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "remarkConfig": {
    "plugins": [
      [
        "frontmatter",
        {
          "type": "yaml",
          "marker": "-"
        }
      ],
      "remark-preset-lint-recommended",
      [
        "remark-lint-no-literal-urls",
        false
      ]
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/tests/__setup__/transformer.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "<rootDir>/tests/__setup__/frameworkSetup.js",
    "setupFiles": [
      "<rootDir>/tests/__setup__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
