{
  "name": "www-gavinmogan-com",
  "private": true,
  "description": "My Website powered by gatsby",
  "version": "0.0.0",
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halkeye/www-gavinmogan-com.git"
  },
  "bugs": {
    "url": "https://github.com/halkeye/www-gavinmogan-com/issues"
  },
  "homepage": "https://www.gavinmogan.com",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/react-fontawesome": "0.0.20",
    "autobind": "^1.0.3",
    "babel-plugin-lodash": "^3.3.2",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "gatsby-image": "^1.0.51",
    "gatsby-link": "^1.6.44",
    "gatsby-plugin-canonical-urls": "^1.0.18",
    "gatsby-plugin-catch-links": "^1.0.22",
    "gatsby-plugin-feed": "^1.3.22",
    "gatsby-plugin-google-analytics": "^1.0.31",
    "gatsby-plugin-manifest": "^1.0.22",
    "gatsby-plugin-netlify": "^1.0.19",
    "gatsby-plugin-nprogress": "^1.0.14",
    "gatsby-plugin-offline": "^1.0.16",
    "gatsby-plugin-react-helmet": "^2.0.11",
    "gatsby-plugin-robots-txt": "^1.0.2",
    "gatsby-plugin-sass": "^1.0.26",
    "gatsby-plugin-sentry": "0.0.4",
    "gatsby-plugin-sharp": "^1.6.44",
    "gatsby-plugin-sitemap": "^1.2.23",
    "gatsby-plugin-twitter": "^1.0.20",
    "gatsby-remark-autolink-headers": "^1.4.18",
    "gatsby-remark-copy-linked-files": "^1.5.32",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-images": "^1.5.63",
    "gatsby-remark-prismjs": "^2.0.2",
    "gatsby-remark-responsive-iframe": "^1.4.18",
    "gatsby-remark-source-name": "^1.0.0",
    "gatsby-source-filesystem": "^1.5.36",
    "gatsby-transformer-remark": "^1.7.41",
    "gatsby-transformer-sharp": "^1.6.24",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "prismjs": "^1.14.0",
    "react": "^16.3.2",
    "react-disqus-comments": "^1.2.0",
    "react-dom": "^16.3.2",
    "react-gist": "^1.2.1",
    "react-helmet": "^5.2.0",
    "react-md": "^1.3.1",
    "react-share": "^2.1.1",
    "react-twitter-widgets": "^1.7.1",
    "rehype-react": "^3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.3",
    "babel-jest": "^22.4.4",
    "cli-glob": "^0.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "gatsby": "^1.9.261",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^22.4.4",
    "lint-staged": "^7.1.1",
    "markdownlint": "^0.9.0",
    "markdownlint-cli": "^0.9.0",
    "prettier": "^1.12.1",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.3.2",
    "remark-cli": "^5.0.0",
    "remark-frontmatter": "^1.2.0",
    "remark-preset-lint-recommended": "^3.0.1",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.2.1",
    "stylelint-config-standard": "^18.2.0",
    "write-good": "^0.12.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "precommit": "lint-staged",
    "develop": "gatsby develop",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "remarkConfig": {
    "plugins": [
      [
        "frontmatter",
        {
          "type": "yaml",
          "marker": "-"
        }
      ],
      "remark-preset-lint-recommended",
      [
        "remark-lint-no-literal-urls",
        false
      ]
    ]
  },
  "jest": {
    "testPathIgnorePatterns": ["/node_modules/", "<rootDir>/.cache/"],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/test/transformer.js"
    },
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
