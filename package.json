    "gatsby-plugin-netlify": "^2.1.10",
    "gatsby-plugin-netlify-cms": "^4.1.17",
    "gatsby-plugin-nprogress": "^2.1.4",
    "gatsby-plugin-react-helmet": "^3.1.5",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-sass": "^2.1.12",
    "gatsby-plugin-sharp": "^2.2.18",
    "gatsby-plugin-sitemap": "^2.2.9",
    "gatsby-plugin-twitter": "^2.1.4",
    "gatsby-remark-autolink-headers": "^2.1.8",
    "gatsby-remark-copy-linked-files": "^2.1.13",
    "gatsby-remark-images": "^3.1.19",
    "gatsby-remark-prismjs": "^3.3.9",
    "gatsby-remark-responsive-iframe": "^2.2.10",
    "gatsby-source-filesystem": "^2.1.18",
    "gatsby-transformer-remark": "^2.6.19",
    "gatsby-transformer-sharp": "^2.2.12",
    "netlify-cms-app": "^2.9.7"
  },
  "peerDependencies": {
    "gatsby": ">=2"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "develop": "gatsby develop -p 3000 -H 0.0.0.0",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "TZ=UTC gatsby build",
    "build:pp": "TZ=UTC gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint  --ext .js,.jsx --ignore-path .gitignore .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "remarkConfig": {
    "plugins": [
      [
        "frontmatter",
        {
          "type": "yaml",
          "marker": "-"
        }
      ],
      "remark-preset-lint-recommended",
      [
        "remark-lint-no-literal-urls",
        false
      ]
    ]
  },
  "jest": {
    "verbose": true,
    "testURL": "http://localhost/",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "transformIgnorePatterns": [],
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/tests/__setup__/transformer.js"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|css)$": "<rootDir>/tests/__mocks__/fileMock.js",
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFilesAfterEnv": [
      "<rootDir>/tests/__setup__/frameworkSetup.js"
    ],
    "setupFiles": [
      "<rootDir>/tests/__setup__/setup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
