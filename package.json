    "react": "18.2.0",
    "react-dom": "18.2.0"
  },
  "devDependencies": {
    "@babel/core": "7.23.9",
    "@babel/eslint-parser": "7.23.10",
    "@types/node": "18.19.17",
    "@types/react": "^18.0.27",
    "@types/react-dom": "^18.0.10",
    "eslint": "8.56.0",
