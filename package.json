{
  "name": "www-gavinmogan-com",
  "private": true,
  "description": "My Website powered by gatsby",
  "version": "0.0.0",
  "author": "Gavin Mogan <npm@gavinmogan.com> (https://www.gavinmogan.com/)",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halkeye/www-gavinmogan-com.git"
  },
  "bugs": {
    "url": "https://github.com/halkeye/www-gavinmogan-com/issues"
  },
  "homepage": "https://www.gavinmogan.com",
  "dependencies": {
    "@fortawesome/fontawesome": "^1.1.8",
    "@fortawesome/fontawesome-free-brands": "^5.0.13",
    "@fortawesome/fontawesome-free-solid": "^5.0.13",
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "@fortawesome/react-fontawesome": "0.1.0",
    "autobind": "^1.0.3",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "eslint-config-standard": "^11.0.0",
    "gatsby-plugin-favicon": "^2.1.1",
    "gatsby-plugin-robots-txt": "^1.1.0",
    "gatsby-plugin-sentry": "0.1.0",
    "gatsby-remark-embed-youtube": "0.0.7",
    "gatsby-remark-emoji": "0.0.2",
    "gatsby-remark-source-name": "^1.0.0",
    "lodash": "^4.17.10",
    "lodash-webpack-plugin": "^0.11.5",
    "prismjs": "^1.15.0",
    "react": "^16.4.1",
    "react-disqus-comments": "^1.2.0",
    "react-dom": "^16.4.1",
    "react-gist": "^1.2.1",
    "react-helmet": "^5.2.0",
    "react-md": "^1.4.2",
    "react-share": "^2.2.0",
    "react-twitter-widgets": "^1.7.1",
    "rehype-react": "^3.0.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.6",
    "babel-jest": "^23.4.0",
    "babel-preset-env": "^1.7.0",
    "cli-glob": "^0.1.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^5.1.0",
    "eslint-config-prettier": "^3.0.1",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jest": "^21.17.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-node": "^7.0.1",
    "eslint-plugin-promise": "^3.8.0",
    "eslint-plugin-react": "^7.10.0",
    "eslint-plugin-standard": "^3.1.0",
    "gatsby": "^2.0.0-rc.9",
    "gatsby-image": "^2.0.0-rc.1",
    "gatsby-link": "^2.0.0-rc.2",
    "gatsby-plugin-canonical-urls": "^2.0.0-rc.1",
    "gatsby-plugin-catch-links": "^2.0.2-rc.1",
    "gatsby-plugin-feed": "^2.0.0-rc.2",
    "gatsby-plugin-manifest": "^2.0.2-rc.1",
    "gatsby-plugin-netlify": "^2.0.0-rc.1",
    "gatsby-plugin-nprogress": "^2.0.0-rc.1",
    "gatsby-plugin-offline": "^2.0.0-rc.2",
    "gatsby-plugin-react-helmet": "^3.0.0-rc.1",
    "gatsby-plugin-sass": "^2.0.0-rc.1",
    "gatsby-plugin-sharp": "^2.0.0-rc.3",
    "gatsby-plugin-sitemap": "^2.0.0-rc.1",
    "gatsby-plugin-twitter": "^2.0.0-rc.1",
    "gatsby-remark-autolink-headers": "^2.0.0-rc.1",
    "gatsby-remark-copy-linked-files": "^2.0.0-rc.1",
    "gatsby-remark-images": "^2.0.1-rc.1",
    "gatsby-remark-prismjs": "^3.0.0-rc.2",
    "gatsby-remark-responsive-iframe": "^2.0.0-rc.1",
    "gatsby-source-filesystem": "^2.0.1-rc.1",
    "gatsby-transformer-remark": "^2.1.1-rc.1",
    "gatsby-transformer-sharp": "^2.1.1-rc.2",
    "gh-pages": "^1.2.0",
    "husky": "^0.14.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.0",
    "lint-staged": "^7.2.0",
    "markdownlint": "^0.11.0",
    "markdownlint-cli": "^0.13.0",
    "prettier": "^1.13.7",
    "prettier-eslint-cli": "^4.7.1",
    "react-test-renderer": "^16.4.1",
    "remark-cli": "^5.0.0",
    "remark-frontmatter": "^1.2.0",
    "remark-preset-lint-recommended": "^3.0.2",
    "stylefmt": "^6.0.0",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0",
    "write-good": "^0.13.0"
  },
  "keywords": [
    "gatsby"
  ],
  "license": "MIT",
  "main": "n/a",
  "scripts": {
    "precommit": "lint-staged",
    "develop": "gatsby develop -p 3000",
    "dev": "npm run develop",
    "serve": "gatsby serve",
    "build": "gatsby build",
    "build:pp": "gatsby build --prefix-paths",
    "build:gh": "npm run clean && npm run build:pp && gh-pages -d public",
    "clean": "rm -rf public",
    "lint:js": "eslint --ext .js,.jsx .",
    "lint:md": "remark content/posts/",
    "write-good": "write-good $(glob 'content/posts/**/*.md')",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.jsx": [
      "eslint --fix",
      "git add"
    ]
  },
  "remarkConfig": {
    "plugins": [
      [
        "frontmatter",
        {
          "type": "yaml",
          "marker": "-"
        }
      ],
      "remark-preset-lint-recommended",
      [
        "remark-lint-no-literal-urls",
        false
      ]
    ]
  },
  "jest": {
    "testPathIgnorePatterns": [
      "/node_modules/",
      "<rootDir>/.cache/"
    ],
    "globals": {
      "__PATH_PREFIX__": ""
    },
    "transform": {
      "^.+\\.jsx?$": "<rootDir>/test/transformer.js"
    },
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    },
    "setupFiles": [
      "./jestSetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ]
  }
}
